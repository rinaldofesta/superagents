{
  "name": "superagents",
  "version": "1.3.1",
  "description": "Context-aware Claude Code configuration generator - Creates AI-powered agents and skills tailored to your codebase",
  "main": "dist/index.js",
  "type": "module",
  "bin": {
    "superagents": "./bin/superagents"
  },
  "files": [
    "dist",
    "bin",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsc && npm run copy-templates",
    "copy-templates": "mkdir -p dist/templates && cp -r src/templates/agents dist/templates/ && cp -r src/templates/skills dist/templates/",
    "dev": "tsx src/index.ts",
    "dev:watch": "tsx watch --clear-screen=false --ignore 'dist/**' --ignore 'node_modules/**' --ignore '.claude/**' src/index.ts",
    "start": "node dist/index.js",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "lint": "eslint src/",
    "type-check": "tsc --noEmit",
    "clean": "rm -rf dist",
    "prepublishOnly": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Play-New/superagents.git"
  },
  "bugs": {
    "url": "https://github.com/Play-New/superagents/issues"
  },
  "homepage": "https://superagents.playnew.com",
  "keywords": [
    "claude",
    "anthropic",
    "ai",
    "cli",
    "code-generation",
    "agents",
    "skills",
    "configuration",
    "developer-tools"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@anthropic-ai/sdk": "^0.30.0",
    "@clack/prompts": "^0.7.0",
    "@types/archiver": "^7.0.0",
    "@types/unzipper": "^0.10.11",
    "archiver": "^7.0.1",
    "commander": "^12.0.0",
    "cosmiconfig": "^9.0.0",
    "fs-extra": "^11.2.0",
    "glob": "^10.3.0",
    "ora": "^8.0.0",
    "p-limit": "^7.2.0",
    "picocolors": "^1.0.0",
    "unzipper": "^0.12.3",
    "zod": "^3.22.0"
  },
  "devDependencies": {
    "@types/fs-extra": "^11.0.4",
    "@types/node": "^20.10.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.55.0",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3",
    "vitest": "^4.0.18"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}
